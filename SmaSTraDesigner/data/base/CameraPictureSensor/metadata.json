{
  "type": "sensor",
  "name": "CameraPictureSensor",
  "author": "SmaSTraBasic",
  "mainClass": "de.tu_darmstadt.smastra.sensors.hardware.CameraPictureSensor",
  "display": "CameraPictureSensor",
  "description": "Takes a picture by the camera",
  "needs": [
    "de.tu_darmstadt.smastra.sensors.Picture"
  ],
  "config": [],
  "proxyProperties": [
    {
      "type": "android.view.SurfaceView",
      "method": "setTextureView",
      "name": "setSurfaceView"
    }
  ],
  "extras": [
    {
      "type": "permission",
      "permission": "android.permission.CAMERA"
    }
  ],
  "dataMethod": "getLastData",
  "output": "de.tu_darmstadt.smastra.sensors.Picture",
  "start": "start",
  "stop": "stop"
}
